{
  "private": true,
  "name": "remix-app-template-js",
  "description": "",
  "license": "",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -o ./app/tailwind.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/tailwind.css --watch",
    "postinstall": "patch-package && remix setup node",
    "start": "remix-serve build"
  },
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.js"
  },
  "dependencies": {
    "@prisma/client": "^3.9.0",
    "@remix-run/react": "^1.1.3",
    "@remix-run/serve": "^1.1.3",
    "file-type": "^17.1.1",
    "front-matter": "^4.0.2",
    "highlight.js": "^11.4.0",
    "jsdom": "^19.0.0",
    "markdown-it": "^12.3.2",
    "mathjax-full": "^3.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3",
    "transliteration": "^2.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@types/markdown-it": "^12.2.3",
    "concurrently": "^7.0.0",
    "esbuild-register": "^3.3.2",
    "patch-package": "^6.4.7",
    "prisma": "^3.9.0",
    "tailwindcss": "^3.0.18"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
